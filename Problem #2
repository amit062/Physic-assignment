#include<bits/stdc++.h>
using namespace std;
int main() {
    int m1=3,m2=2;
    double lambda1=530.0,lambda2=700.0;
    double theta1=65.0;

    double d=(m1*lambda1)/(sin(theta1*M_PI/180));

    double theta2=asin((m2 * lambda2) / d);


    theta2=theta2*(180/M_PI);

    cout<<fixed<<setprecision(2)<<"The angle for the second-order bright spot for red light: "<<theta2;

    
}
